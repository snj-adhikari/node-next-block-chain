{
  "version": 2,
  "name": "blockchain-generator",
  "functions": {
    "api/backend.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/backend"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/backend"
    }
  ]
}
