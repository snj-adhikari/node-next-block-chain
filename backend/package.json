{
  "name": "blockchain-generator-backend",
  "version": "1.0.0",
  "description": "Backend API for blockchain generator",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
